<h1>Create Account</h1>

<%= form_for(@user) do |f| %>

<div class="row">

<%= f.text_field :user_name %>
<%= f.label :user_name %>
</div>
<div class="row">

<%= f.password_field :password %>
<%= f.label :password %>
</div>
<div class="row">

<%= f.password_field :password_confirmation %>
<%= f.label :password_confirmation %>
</div>
<div class="row">
<%= f.submit %>
</div>
<% end %>
